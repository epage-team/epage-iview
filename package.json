{
  "name": "epage-iview",
  "version": "0.2.1",
  "description": "epage render package based on iview component library",
  "author": "Chengzi <ttghost@126.com>",
  "main": "./dist/epage-iview.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "prod": "cross-env NODE_ENV=production webpack --config build/webpack.build.conf.js && npm run style",
    "style": "cross-env NODE_ENV=production webpack --config build/webpack.style.js",
    "deploy": "npm run prod && npm publish",
    "lint": "eslint --ext .vue --ext .js src/ --fix",
    "analyz": "npm_config_report=true npm run prod"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epage-team/epage-iview.git"
  },
  "keywords": [
    "epage",
    "epage-iview",
    "render"
  ],
  "bugs": {
    "url": "https://github.com/epage-team/epage-iview/issues"
  },
  "homepage": "https://github.com/epage-team/epage-iview#readme",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add .",
      "commit-msg": "node build/verify-commit-msg.js"
    }
  },
  "dependencies": {
    "epage": "^0.2.1",
    "vue": "^2.5.16",
    "vuedraggable": "^2.20.0",
    "vuex": "^3.1.2"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-plugin-transform-vue-jsx": "^1.1.2",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^7.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "iview": "^2.13.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "quill": "^1.3.7",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "url-loader": "^1.1.2",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
